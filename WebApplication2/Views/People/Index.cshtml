@model IEnumerable<Person>

<h2 class="text-center">List of People</h2>


<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>

        </tr>
    </thead>
    <tbody>

        @foreach (Person item in Model)
        {
            <tr>
                <th scope="row">1</th>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>

            </tr>
        }



    </tbody>
</table>